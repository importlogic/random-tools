<html lang="en" class="preload">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Favicon  -->
    <link rel="icon" href="https://cdn.jsdelivr.net/gh/importlogic/jsdelivr@main/random-tools/public/images/favicon.ico">

    <!-- Bootstrap -->
    <link rel="stylesheet" href="" id="stylesBootstrap">

    <!-- Common CSS  -->
    <link rel="stylesheet" href="/css/styles.css" id="stylesCommon">

    <!-- Switching Theme  -->
    <link rel="stylesheet" href="" id="stylesTheme">

    <!-- Navbar  -->
    <div class="container container-fluid">
      <nav class="navbar navbar-expand-lg navbar-light">
          <div class="container-fluid">
            <a class="navbar-brand" href="/"><img src="https://cdn.jsdelivr.net/gh/importlogic/jsdelivr@main/random-tools/public/images/icon-transparent.webp" id="navBarIcon"></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor03" aria-controls="navbarColor03" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
        
            <div class="collapse navbar-collapse" id="navbarColor03">
              <ul class="navbar-nav ms-auto">
                <li class="nav-item">
                  <a class="nav-link home-link" href="/">Home
                    <span class="visually-hidden">(current)</span>
                  </a>
                </li>
                <li class="nav-item">
                  <a class="nav-link about-link" href="/about">About</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link contact-link" href="/contact">Contact</a>
                </li>
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle tools-link" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Tools</a>
                  <div class="dropdown-menu">
                    <% for(var i = 0; i < Math.min(toolsList.length, 4); i++) { %>
                      <a class="dropdown-item" href=<%= toolsList[i].link %>><%= toolsList[i].name %></a>
                    <% } %>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="/tools">All Tools</a>
                  </div>
                </li>
                <li class="nav-item">
                  <input type="checkbox" id="changeMode">
                </li>
              </ul>
            </div>
          </div>
      </nav>
    </div>